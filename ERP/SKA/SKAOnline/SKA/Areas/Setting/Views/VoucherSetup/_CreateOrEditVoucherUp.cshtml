@model SKA.Areas.Setting.Models.ViewModels.VoucherSetupViewModel

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <table>
        <tr>
            <td>Dibuat Oleh</td>
            <td>:</td>
            <td>
                @Html.TextBoxFor(model => model.MakerName)
                @Html.ValidationMessageFor(model => model.MakerName)
            </td>
        </tr>
        <tr>
            <td>Jabatan</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.MakerPosition)
                @Html.ValidationMessageFor(model => model.MakerPosition)
            </td>
        </tr>
        <tr>
            <td>Diteliti Oleh</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.InvestigatorName)
                @Html.ValidationMessageFor(model => model.InvestigatorName)
            </td>
        </tr>
        <tr>
            <td>Jabatan</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.InvestigatorPosition)
                @Html.ValidationMessageFor(model => model.InvestigatorPosition)
            </td>
        </tr>
        <tr>
            <td>Diperiksa Oleh</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.ExaminerName)
                @Html.ValidationMessageFor(model => model.ExaminerName)
            </td>
        </tr>
        <tr>
            <td>Jabatan</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.ExaminerPosition)
                @Html.ValidationMessageFor(model => model.ExaminerPosition)
            </td>
        </tr>
        <tr>
            <td>Direksi I</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.FirstAdministrationName)
                @Html.ValidationMessageFor(model => model.FirstAdministrationName)
            </td>
        </tr>
        <tr>
            <td>Direksi II</td>
            <td>:</td>
            <td>
                @Html.EditorFor(model => model.SecondAdministrationName)
                @Html.ValidationMessageFor(model => model.SecondAdministrationName)
            </td>
        </tr>
    </table>
    <p>
        <input type="submit" value="Simpan" class="t-button" style ="width:60px" />
        &nbsp;
        <input type="reset" value="Reset" class="t-button" style ="width:60px" />
        &nbsp;
        <a class ="t-button" style ="width:60px" href="@Url.Action("IndexVoucherUp", "VoucherSetup", null)">Kembali</a>
     </p>
}
